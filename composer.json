{
    "name": "kerryrandolph/modern-wp-plugin",
    "description": "A modern starting point for building a Wordpress plugin",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "plugin"],
    "homepage": "https://github.com/Kerry-Randolph/modern-wp-plugin-jumpstart",
    "license": "MIT",
    "authors": [
        {
            "name": "Kerry Randolph",
            "email": "kerrywritescode@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "php-di/php-di": "^6.0",
        "kerryrandolph/wp-hook-annotations": "^0.0.3",
        "htmlburger/carbon-fields": "^3.1",
        "psr/container": "^1.0",
        "twig/twig": "^2.11",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.24",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "<8"
    },
    "autoload": {
        "psr-4": {
            "UniqueRootNamespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UniqueRootNamespace\\Tests\\": "tests/"
        }
    }
}
